            $(document).ready(function () {
                //$('.timepicker').datetimepicker({ datepicker: false, format: 'H:i' });
                $('#plYellowCard').hide();
                $('#plAdmin').hide();
                $('#plUSB').hide();

                // subscribe to change events
                $('#YellowCard').change(function () {
                    RunsOnMondays();
                });
                $('#USB').change(function () {
                    RunsOnPlUSB();
                });
                $('#AdminAc').change(function () {
                    RunsOnPlAdmins();
                });
                $('#Port').change(function () {
                    RunsOnPlPort();
                });
                $('#OpenAccess').change(function () {
                    RunsOnPlOpenAccess();
                });
            });

            function RunsOnMondays() {
                if ($('#YellowCard').prop('checked')) {
                    $('#plYellowCard').slideDown();
                } else {
                    $('#plYellowCard').slideUp();
                }
            }

            function RunsOnPlUSB() {
                if ($('#USB').prop('checked')) {
                    $('#plAdmin').slideDown();
                    $('#plUSB').slideDown();
                } else {
                    $('#plAdmin').slideUp();
                    $('#plUSB').slideUp();
                }
            }

            function RunsOnPlAdmins() {
                if ($('#AdminAc').prop('checked')) {
                    $('#plAdmin').slideDown();
                } else {
                    $('#plAdmin').slideUp();
                }
            }

            function RunsOnPlPort() {
                if ($('#Port').prop('checked')) {
                    $('#plAdmin').slideDown();
                } else {
                    $('#plAdmin').slideUp();
                }
            }

            function RunsOnPlOpenAccess() {
                if ($('#OpenAccess').prop('checked')) {
                    $('#plAdmin').slideDown();
                } else {
                    $('#plAdmin').slideUp();
                }
            }

        </script>

/* ---------------------------------------------------
    MEDIAQUERIES
----------------------------------------------------- */
@media (max-width: 768px) {
    #page-content-wrapper{
        padding-left:0px;
    }
    #sidebar {
        margin-left: -250px;
        
    }
    #sidebar.active {
        margin-left: 0;
    }
    
    #sidebarCollapse span {
        display: none;
    }
}